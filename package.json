{
  "name": "react-expess-fullstack-boilerplate",
  "version": "1.0.0",
  "description": "Fullstack react-express boilerplate",
  "main": "server/server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm run build && npm prune --production",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Nikolay Milya",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "nodemon": "^1.14.12"
  }
}
